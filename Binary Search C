#include <stdio.h>

int main() {
    int a[] = {2, 4, 6, 8, 10, 12, 14};
    int n = sizeof(a)/sizeof(a[0]);
    int key = 10;
    int low = 0, high = n-1, mid;
    int pos = -1;

    while(low <= high){
        mid = (low + high)/2;
        if(a[mid] == key){
            pos = mid;
            break;
        } else if(a[mid] < key){
            low = mid + 1;
        } else {
            high = mid - 1;
        }
    }

    if(pos != -1) printf("Element found at index %d", pos);
    else printf("Element not found");
    return 0;
}
