#include <stdio.h>

int main() {
    int a[] = {64, 25, 12, 22, 11};
    int n = sizeof(a)/sizeof(a[0]);
    int i, j, temp;

    for(i=0;i<n-1;i++){
        for(j=0;j<n-1-i;j++){
            if(a[j] > a[j+1]){
                temp = a[j];
                a[j] = a[j+1];
                a[j+1] = temp;
            }
        }
    }

    printf("Sorted array: ");
    for(i=0;i<n;i++) printf("%d ", a[i]);
    return 0;
}
